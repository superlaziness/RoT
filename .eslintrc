{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },

  "globals": {
    "__DEV__": false,
    "__BROWSER__": false,
    "__NODE__": false,
    "__RASPI__": false,
  },

  "rules": {
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "func-names": 0,
    "camelcase": 0,
    "quote-props": 0,
    "consistent-return": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/forbid-prop-types": 0,
    "react/no-unused-prop-types": [2, { "skipShapeProps": true }],
    "import/no-extraneous-dependencies": 0,
    "no-plusplus": 0,
    "react/sort-comp": 0,
    "react/no-did-mount-set-state": 0,
    "no-mixed-operators": 0,
    "no-underscore-dangle": 0,
    "arrow-body-style": 0,
    "react/no-danger": 0,
    "arrow-parens": 0,
    "no-unused-expressions": 0,
  },

  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
